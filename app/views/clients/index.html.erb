<h1>Listing clients</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Birth date</th>
      <th>Race</th>
      <th>Register date</th>
      <th>Register location name</th>
      <th>Gender</th>
      <th>Veterancy</th>
      <th>Neighborhood</th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= link_to client.code, client %></td>
        <td><%= client.birth_date %></td>
        <td><%= client.race %></td>
        <td><%= client.register_date %></td>
        <td><%= Location.find(client.register_location_name).name %></td>
        <td><%= client.gender %></td>
        <td><%= client.veterancy %></td>
        <td><%= client.neighborhood %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
